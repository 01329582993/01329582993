#include<stdio.h>
    int main (){
        int num;
    printf("Enter the number :");
    scanf("%d",&num);
    if(num>0){
        for (int i=num;i<=(num/2);i++){

        if(num%i==0){
    printf("%d is no prime ",num);

}
        else{
    printf("%d is primr number",num);
}
}
    }
    return 0;
    }

